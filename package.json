{
  "name": "wly-ui",
  "version": "1.1.0",
  "description": "A React UI Library.",
  "homepage": "https://github.com/serendipityApe/wly-ui#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "typings": "lib/index.d.ts",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "authors": {
    "name": "serendipityApe",
    "email": "1577142422@qq.com"
  },
  "files": [
    "lib",
    "esm"
  ],
  "scripts": {
    "dev": "dumi dev",
    "start": "npm run dev",
    "build:site": "rimraf doc-site && dumi build",
    "preview:site": "cross-env SITE_BUILD_ENV=PREVIEW npm run build:site && serve doc-site",
    "deploy:site": "npm run build:site && gh-pages -d doc-site",
    "clean": "rimraf lib esm dist",
    "build:types": "tsc -p tsconfig.build.json && cpr lib esm",
    "build": "npm run clean && npm run build:types && gulp",
    "prepublishOnly": "npm run build",
    "release": "np --no-cleanup --no-tests --any-branch",
    "commit": "git-cz",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "new": "plop --plopfile ./scripts/plopfile.ts",
    "postnew": "prettier --write src/**/*{ts,tsx} --loglevel silent",
    "eslint-fix": "eslint --fix --ext .js,.ts,.tsx src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serendipityApe/wly-ui.git"
  },
  "keywords": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/serendipityApe/wly-ui/issues"
  },
  "lint-staged": {
    "src/**/*.ts?(x)": [
      "prettier --write",
      "eslint --fix",
      "jest --bail --findRelatedTests",
      "git add"
    ],
    "src/**/*.less": [
      "stylelint --syntax less --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^24.0.25",
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.8.0",
    "@types/testing-library__react": "^9.1.2",
    "@typescript-eslint/parser": "^1.9.0",
    "@umijs/fabric": "^1.2.12",
    "babel-plugin-import": "^1.13.3",
    "commitizen": "^4.0.3",
    "cpr": "^3.0.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "doctoc": "^1.4.0",
    "dumi": "^1.0.13",
    "gh-pages": "^3.2.3",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-less": "^4.0.1",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "less": "^3.10.3",
    "lint-staged": "^9.5.0",
    "np": "^7.5.0",
    "plop": "^2.6.0",
    "prettier": "^1.19.1",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "rimraf": "^3.0.0",
    "serve": "^12.0.0",
    "snapshot-diff": "^0.6.1",
    "through2": "^3.0.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "prop-types": "^15.7.2"
  },
  "resolutions": {
    "@types/react": "16.14.10",
    "@types/react-dom": "16.9.13"
  }
}
